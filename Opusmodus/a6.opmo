(setf omn0 '(-8/4))
(setf omn1 '(-7/4))
(setf pitch (gen-repeat 3 'd4 ))
(setf length (stretch 6/4 pitch ))
(setf omn2 (make-omn :pitch pitch :length length))
(setf omn (assemble-seq '(omn0 omn1 omn2 )))
(def-score a6
         (:key-signature 'atonal
          :time-signature (get-time-signature omn)
          :tempo 120)
(a6 :omn omn)
)