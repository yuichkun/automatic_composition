
(defun stretch (dur pitch)
  (let '(ret nil) '(pitch-len nil)
    (setf pitch-len (length (flatten pitch)))
    (setf ret (gen-repeat pitch-len (/ dur pitch-len)))
    (return-from stretch ret)
  )
)
